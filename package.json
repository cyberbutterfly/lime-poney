{
  "name": "@2pg/lime-poney",
  "version": "1.0.3",
  "author": "Pierre Chausalet <pierre.chaussalet@twoporeguys.com>",
  "description": "2PG react components library",
  "homepage": ".",
  "main": "./index.js",
  "types": "./index.d.ts",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/enzyme": "^3.1.3",
    "@types/jest": "^21.1.5",
    "@types/leaflet": "^1.2.0",
    "@types/lodash": "^4.14.80",
    "@types/plotly.js": "^1.28.4",
    "@types/react": "^16.0.22",
    "@types/react-dom": "^16.0.3",
    "@types/react-leaflet": "^1.1.4",
    "@types/react-native": "^0.49.2",
    "@types/react-native-vector-icons": "^4.4.1",
    "@types/react-router-dom": "^4.0.8",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.2.2",
    "fast-sass-loader": "^1.4.0",
    "file-loader": "^1.1.5",
    "jest": "^21.2.1",
    "jest-serializer-enzyme": "^1.0.0",
    "node-sass": "^4.5.3",
    "resolve-url-loader": "^2.1.1",
    "source-map-loader": "^0.2.2",
    "style-loader": "^0.19.0",
    "svg-sprite-loader": "^3.4.1",
    "ts-jest": "^21.0.0",
    "ts-loader": "^2.3.7",
    "typescript": "^2.6.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.9.0"
  },
  "dependencies": {
    "leaflet": "^1.2.0",
    "lodash": "^4.17.4",
    "plotly.js": "^1.30.0",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-leaflet": "^1.7.0",
    "react-native-vector-icons": "^4.4.0",
    "react-native-web": "^0.1.12",
    "react-router-dom": "^4.2.2",
    "rxjs": "^5.5.0"
  },
  "jest": {
    "testRegex": "(/_tests_/.*|\\.Test)\\.(tsx)$",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/index.{ts,js}",
      "!**/*.d.ts"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/_tests_/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/_tests_/__mocks__/styleMock.js",
      "react-native": "<rootDir>/node_modules/react-native-web"
    },
    "setupFiles": [
      "<rootDir>/_tests_/_raf_shim_.ts",
      "<rootDir>/_tests_/_setup_.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/_tests_/__mocks__/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/src",
      "<rootDir>/node_modules/chai-enzyme",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ]
  },
  "scripts": {
    "test": "./node_modules/jest/bin/jest.js",
    "test:watch": "./node_modules/jest/bin/jest.js --watch",
    "cov": "open ./coverage/lcov-report/index.html"
  }
}
