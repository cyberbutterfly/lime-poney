  "homepage": ".",
  "main": "./index.js",
  "types": "./index.d.ts",
  "devDependencies": {
    "@types/leaflet": "^1.2.0",
    "@types/lodash": "^4.14.78",
    "@types/plotly.js": "^1.28.7",
    "@types/react": "^16.0.16",
    "@types/react-dom": "^16.0.2",
    "@types/react-leaflet": "^1.1.4",
    "@types/react-native": "^0.49.2",
    "@types/react-native-vector-icons": "^4.4.1",
    "@types/react-onsenui": "^2.1.4",
    "@types/react-router": "^4.0.16",
    "@types/react-router-dom": "^4.0.8",
    "css-loader": "^0.28.7",
    "fast-sass-loader": "^1.3.0",
    "file-loader": "^1.1.5",
    "node-sass": "^4.5.3",
    "resolve-url-loader": "^2.1.1",
    "source-map-loader": "^0.2.2",
    "style-loader": "^0.19.0",
    "svg-sprite-loader": "^3.4.0",
    "ts-loader": "^3.0.5",
    "typescript": "^2.5.3",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "dependencies": {
    "leaflet": "^1.2.0",
    "onsenui": "^2.6.1",
    "plotly.js": "^1.31.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-leaflet": "^1.7.1",
    "react-native-vector-icons": "^4.4.2",
    "react-native-web": "^0.1.12",
    "react-onsenui": "^1.7.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "rxjs": "^5.5.0"
  },
  "jest": {
    "testRegex": "(/_tests_/.*|\\.Test)\\.(tsx)$",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/_tests_/__mocks__/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/src",
      "<rootDir>/node_modules/chai-enzyme",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/_tests_/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/_tests_/__mocks__/styleMock.js"
    }
  }
}
