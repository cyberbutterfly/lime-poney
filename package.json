{
  "name": "@2pg/lime-poney",
  "version": "0.8.0",
  "author": "Pierre Chausalet <pierre.chaussalet@twoporeguys.com>",
  "description": "2PG react components library",
  "homepage": ".",
  "main": "./index.js",
  "types": "./index.d.ts",
  "devDependencies": {
    "@types/leaflet": "^1.2.0",
    "@types/plotly.js": "^1.28.4",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-leaflet": "^1.1.4",
    "@types/react-native": "^0.48.6",
    "@types/react-native-vector-icons": "^4.3.2",
    "@types/react-onsenui": "^2.1.3",
    "jest": "^21.0.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "svg-sprite-loader": "^3.2.5",
    "ts-jest": "^21.0.0",
    "ts-loader": "^2.3.4",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "leaflet": "^1.2.0",
    "lodash": "^4.17.4",
    "onsenui": "^2.5.3",
    "plotly.js": "^1.30.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-leaflet": "^1.6.4",
    "react-native-vector-icons": "^4.4.0",
    "react-native-web": "^0.0.126",
    "react-onsenui": "^1.5.0",
    "rxjs": "^5.4.3"
  },
  "jest": {
    "testRegex": "(/_tests_/.*|\\.Test)\\.(tsx)$",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/_tests_/__mocks__/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/src",
      "<rootDir>/node_modules/chai-enzyme",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/_tests_/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/_tests_/__mocks__/styleMock.js"
    }
  }
}
